00D000  2               RECCHKSUM	= RECTYPE + s_byte	; $76 - Record checksum accumulator
00D000  2               DLFAIL		= RECCHKSUM + s_byte	; $77 - Flag for download failure
00D000  2               TEMP		= DLFAIL + s_byte	; $78 - Save temporary hex value
