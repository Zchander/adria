00D865  2  3B           	tsc				; get current SP (Stack Pointer)
00D866  2  85 8A        	sta	reg_spx
00D868  2  C2 FF        	rep	#%11111111		; Clear SR (Processor Status)
00D86A  2  EA           	nop				; Added two NOPs after REP
00D86B  2  EA           	nop
